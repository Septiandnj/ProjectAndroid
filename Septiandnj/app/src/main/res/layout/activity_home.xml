<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_home"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="assalaam.layout.id.septian.Home">

    <Button
        android:layout_width="fill_parent"
        android:layout_height="55px"
        android:text="Linear Layout Sederhana"
        android:id="@+id/LinearLayout">
    </Button>
    <Button
        android:layout_width="fill_parent"
        android:layout_height="55px"
        android:text="Linear Layout Relative"
        android:id="@+id/LayoutSederhana">
    </Button>
    <Button
        android:layout_width="fill_parent"
        android:layout_height="55px"
        android:text="Linear Layout Table"
        android:id="@+id/TampilanLayoutTabel">
    </Button>
    <Button
        android:layout_width="fill_parent"
        android:layout_height="55px"
        android:text="Menampilkan Gambar Android"
        android:id="@+id/GambarAndroid">
    </Button>
    <Button
        android:layout_width="fill_parent"
        android:layout_height="55px"
        android:text="Teks Autocomplete"
        android:id="@+id/AutoCompleteSederhana">
    </Button>
    <Button
        android:layout_width="fill_parent"
        android:layout_height="55px"
        android:text="Kotak Dialog"
        android:id="@+id/DialogBox">
    </Button>
    <Button
        android:layout_width="fill_parent"
        android:layout_height="55px"
        android:text="Picker"
        android:id="@+id/picker">
    </Button>
    <Button
        android:layout_width="fill_parent"
        android:layout_height="55px"
        android:text="CheckBox"
        android:id="@+id/checkBox">
    </Button>
    <Button
        android:layout_width="fill_parent"
        android:layout_height="55px"
        android:text="Radio Button"
        android:id="@+id/RadioButton">
    </Button>
    <Button
        android:layout_width="fill_parent"
        android:layout_height="55px"
        android:text="Selection Widget"
        android:id="@+id/Seleksi">
    </Button>
    <Button
        android:layout_width="fill_parent"
        android:layout_height="55px"
        android:text="Service Sederhana"
        android:id="@+id/ServiceSederhana">
    </Button>
</LinearLayout>
